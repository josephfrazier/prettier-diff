{
  "name": "prettier-diff",
  "version": "0.0.3",
  "description": "git difftool for semantic JavaScript diffs",
  "main": "index.js",
  "bin": "bin/prettier-diff",
  "files": [
    "bin/prettier-diff",
    "index.js"
  ],
  "scripts": {
    "test": "test/test.sh",
    "diff": "git difftool --extcmd=./bin/prettier-diff",
    "format": "prettier --single-quote --trailing-comma es5 --write index.js bin/prettier-diff"
  },
  "repository": "https://github.com/josephfrazier/prettier-diff.git",
  "author": "Joseph Frazier <1212jtraceur@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "diff": "^3.2.0",
    "diff-so-fancy": "^0.11.4",
    "is-path-global": "^2.0.0",
    "json-stable-stringify": "^1.0.1",
    "prettier": "^0.22.0",
    "shell-escape-tag": "^1.1.0",
    "tempfile": "^2.0.0",
    "yargs": "^7.0.2"
  },
  "devDependencies": {
    "uglify-js-harmony": "^2.7.5"
  }
}
